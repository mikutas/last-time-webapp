<%= bootstrap_form_for OccurredDate.new, url: new_date_path do |f| %>
  <div class="field">
    <%= f.text_field :created_at,
      label: 'Date',
      class: 'datetimepicker',
      data: { :date_format => 'YYYY/MM/DD HH:mm:ss' },
      placeholder: 'YYYY/MM/DD HH:mm:ss'
    %>
  </div>
  <%= f.submit "Add date", class: "btn btn-primary" %>
<% end %>
