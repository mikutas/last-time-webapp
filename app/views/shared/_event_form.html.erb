<%= bootstrap_form_for Event.new do |f| %>
  <div class="field">
    <%= f.text_field :title, placeholder: 'Event title' %>
    <%= f.text_field :created_at,
      label: 'Date',
      class: 'datetimepicker',
      data: { :date_format => 'YYYY/MM/DD HH:mm:ss' },
      placeholder: 'YYYY/MM/DD HH:mm:ss'
    %>
  </div>
  <%= f.submit "Create event", class: "btn btn-primary" %>
<% end %>
