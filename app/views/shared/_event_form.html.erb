<%= form_for Event.new, url: user_events_path(user.id) do |f| %>
  <%= f.text_field :title, placeholder: 'Event title', class: "form-control" %>
  <%= render 'shared/datetimepicker', {f: f} %>
  <%= f.button class: 'btn btn-secondary' do %>
    <i class="fa fa-pencil"> Create event</i>
  <% end %>
<% end %>
