<%= bootstrap_form_for History.new, url: user_event_histories_path(event.user.id, event.id) do |f| %>
  <div class="field">
    <%= render 'shared/datetimepicker', {f: f} %>
  </div>
  <%= f.hidden_field :event_id, :value => event.id %>
  <%= f.submit "Add date", class: "btn btn-primary" %>
<% end %>
