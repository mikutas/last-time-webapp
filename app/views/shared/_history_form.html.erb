<%= form_with model: History.new, url: user_event_histories_path(event.user.id, event.id) do |f| %>
  <%= render "shared/datetimepicker", {f: f} %>
  <%= f.hidden_field :event_id, value: event.id %>
  <%= f.button class: "btn btn-secondary" do %>
    <span class="fa fa-plus"> Add date</span>
  <% end %>
<% end %>
